# Example environment configuration file
# Copy this to .env and fill in your actual API keys
# NEVER commit .env file to version control!

# Google Maps API Key
MAPS_API_KEY=your_google_maps_api_key_here

# Firebase API Keys (platform-specific)
FIREBASE_ANDROID_API_KEY=your_firebase_android_api_key_here
FIREBASE_IOS_API_KEY=your_firebase_ios_api_key_here
FIREBASE_WEB_API_KEY=your_firebase_web_api_key_here
FIREBASE_WINDOWS_API_KEY=your_firebase_windows_api_key_here
FIREBASE_MACOS_API_KEY=your_firebase_macos_api_key_here

# Build Environment
DEBUG=true
PRODUCTION=false

# API Base URL
API_BASE_URL=https://your-api.com
